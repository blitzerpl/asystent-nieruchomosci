{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowedOrigins": [],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
